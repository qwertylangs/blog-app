{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "styled-components": false,
  "component": {
    "shared": {
      "customTemplates": {
        "component": "templates/shared/TemplateName.tsx",
        "story":  "templates/shared/TemplateName.stories.tsx",
        "style": "templates/shared/TemplateName.module.scss"
      },
      "path": "src/shared/ui",
      "withLazy": false,
      "withStory": true,
      "withStyle": true,
      "withTest": false
    },
    "entity": {
      "customTemplates": {
        "component": "templates/shared/TemplateName.tsx",
        "story":  "templates/entities/TemplateName.stories.tsx",
        "style": "templates/shared/TemplateName.module.scss"
      },
      "path": "src/entities",
      "withLazy": false,
      "withStory": true,
      "withStyle": true,
      "withTest": false
    },
    "feature": {
      "customTemplates": {
        "component": "templates/shared/TemplateName.tsx",
        "story":  "templates/features/TemplateName.stories.tsx",
        "style": "templates/shared/TemplateName.module.scss"
      },
      "path": "src/entities",
      "withLazy": false,
      "withStory": true,
      "withStyle": true,
      "withTest": false
    },
    "page": {
      "customTemplates": {
        "component": "templates/shared/TemplateName.tsx",
        "story":  "templates/pages/TemplateName.stories.tsx",
        "style": "templates/shared/TemplateName.module.scss"
      },
      "path": "src/pages",
      "withLazy": false,
      "withStory": true,
      "withStyle": true,
      "withTest": false
    },
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": true,
      "withLazy": false
    }
  },
  "customTemplates": {
    "component": "templates/TemplateName.js",
    "story":  "templates/TemplateName.story.js",
    "style": "templates/TemplateName.module.scss"
  },
  "usesStyledComponents": false
}